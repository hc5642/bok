					<xs:element ref="FedwireFundsAccountReportingRequest" />
					<xs:element ref="FedwireFundsMessageReject" />
                    <xs:element ref="FedwireFundsInvestigationRequest" />
                    <xs:element ref="FedwireFundsInvestigationResponse" />
                </xs:choice>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
