    <xs:element name="FedwireFundsIncoming">
        <xs:complexType>
            <xs:sequence>
            	<xs:element ref="FedwireFundsTechnicalHeader" minOccurs="0" />
                <xs:element ref="FedwireFundsIncomingMessage" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="FedwireFundsTechnicalHeader">
        <xs:complexType>
            <xs:sequence>
                <xs:any minOccurs="0" maxOccurs="unbounded" processContents="lax" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="FedwireFundsIncomingMessage">
        <xs:complexType>
