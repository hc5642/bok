                </xs:annotation>
            </xs:element>
            <xs:element name="TtlCdtNtries" type="NumberAndSumOfTransactions1__1">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">TotalCreditEntries</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Specifies the total number and sum of credit entries.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TtlDbtNtries" type="NumberAndSumOfTransactions1__1">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">TotalDebitEntries</xs:documentation>
