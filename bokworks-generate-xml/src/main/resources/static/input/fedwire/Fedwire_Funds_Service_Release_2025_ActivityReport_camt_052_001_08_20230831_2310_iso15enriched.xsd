                </xs:annotation>
            </xs:element>
            <xs:element name="BkTxCd" type="BankTransactionCodeStructure4__2">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">BankTransactionCode</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Set of elements used to fully identify the type of underlying transaction resulting in an entry.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AddtlInfInd" type="MessageIdentification2__1">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">AdditionalInformationIndicator</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Indicates whether the underlying transaction details are provided through a separate message, as in the case of aggregate bookings.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NtryDtls" type="EntryDetails9__1">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">EntryDetails</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Provides details on the entry.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ReportTiming_FRS_1">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">ReportTiming_FRS_1</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">This indicates whether the report is sent intraday or at the end of day.</xs:documentation>
        </xs:annotation>
